@startuml

class Record {
    + StartTime : DateTime
    + EndTime : DateTime?
    + IsDone : bool
    + Title : string
    + Notes : List<Note>
    + Date : DateOnly
    + Project : Project
    + Id : long
}

class Project {
    + Name :string
    + Id : int
}

class Note {
    + Text : string
    + Id : int
}

Record --> Project
Record "1"  --> "*" Note

@enduml

